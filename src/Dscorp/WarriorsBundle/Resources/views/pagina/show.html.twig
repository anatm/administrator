{% extends '::base.html.twig' %}

{% block body -%}
    <h1>pagina</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titulopagina</th>
                <td>{{ entity.tituloPagina }}</td>
            </tr>
            <tr>
                <th>Urlimagenpagina</th>
                <td>{{ entity.urlimagenPagina }}</td>
            </tr>
            <tr>
