<?php

namespace Dscorp\WarriorsBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * calendario
 */
class calendario
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $diaCalendario;

    /**
     * @var string
     */
    private $mesCalendario;

    /**
     * @var string
     */
    private $anioCalendario;

    /**
     * @var string
     */
    private $tituloCalendario;

    /**
     * @var string
     */
    private $descripcionCalendario;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set diaCalendario
     *
     * @param string $diaCalendario
     * @return calendario
     */
    public function setDiaCalendario($diaCalendario)
    {
        $this->diaCalendario = $diaCalendario;

        return $this;
    }

    /**
     * Get diaCalendario
     *
     * @return string 
     */
    public function getDiaCalendario()
    {
        return $this->diaCalendario;
    }

    /**
     * Set mesCalendario
     *
     * @param string $mesCalendario
     * @return calendario
     */
    public function setMesCalendario($mesCalendario)
    {
        $this->mesCalendario = $mesCalendario;

        return $this;
    }

    /**
     * Get mesCalendario
     *
     * @return string 
     */
    public function getMesCalendario()
    {
        return $this->mesCalendario;
    }

    /**
     * Set anioCalendario
     *
     * @param string $anioCalendario
     * @return calendario
     */
    public function setAnioCalendario($anioCalendario)
    {
        $this->anioCalendario = $anioCalendario;

        return $this;
    }

    /**
     * Get anioCalendario
     *
     * @return string 
     */
    public function getAnioCalendario()
    {
        return $this->anioCalendario;
    }

    /**
     * Set tituloCalendario
     *
     * @param string $tituloCalendario
     * @return calendario
     */
    public function setTituloCalendario($tituloCalendario)
    {
        $this->tituloCalendario = $tituloCalendario;

        return $this;
    }

    /**
     * Get tituloCalendario
     *
     * @return string 
     */
    public function getTituloCalendario()
    {
        return $this->tituloCalendario;
    }

    /**
     * Set descripcionCalendario
     *
     * @param string $descripcionCalendario
     * @return calendario
     */
    public function setDescripcionCalendario($descripcionCalendario)
    {
        $this->descripcionCalendario = $descripcionCalendario;

        return $this;
    }

    /**
     * Get descripcionCalendario
     *
     * @return string 
     */
    public function getDescripcionCalendario()
    {
        return $this->descripcionCalendario;
    }
}
